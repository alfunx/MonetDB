test -n "$YACC" || YACC="yacc"

case "$YACC" in
	bison*)
		# Ubuntu still comes with an ancient bison that uses defines
		# without checking if they are defined.  This triggers a warning
		# from gcc with -Wundef.  If we have a bison < 2.4.3, then
		# disable the warning.
		BISON_VER=`$YACC --version | head -n1 | sed -e 's/^.* //'`
		if test `echo $BISON_VER | sed 's|[_\-][a-zA-Z0-9]*$||' | awk 'BEGIN { FS = "."; } { printf "%d", ($1 * 1000 + $2) * 1000 + $3;}'` -lt `echo 2.4.3 | sed 's|[_\-][a-zA-Z0-9]*$||' | awk 'BEGIN { FS = "."; } { printf "%d", ($1 * 1000 + $2) * 1000 + $3;}'`; then
			X_CFLAGS="`echo ${X_CFLAGS} | sed -e 's/-Wundef//g'`"
		fi
	;;
	*)
		# in embedded mode, we ship the bison-generated files
		if test "x$enable_embedded" = xno; then
			if test "x$enable_sql" = xyes; then
				as_fn_error $? "MonetDB/SQL requires bison" "$LINENO" 5
			fi
			enable_sql=no
			if test "x$disable_sql" = x; then
				disable_sql="(MonetDB/SQL requires bison)"
			fi
		else
			{ $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: ignoring missing bison in embedded configuration" >&5
$as_echo "$as_me: WARNING: ignoring missing bison in embedded configuration" >&2;}
		fi
	;;
esac

INSTALL_BACKUP=""
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking $INSTALL --backup option" >&5
$as_echo_n "checking $INSTALL --backup option... " >&6; }
 if test "$INSTALL"; then
	inst=`echo $INSTALL | sed 's/ .*//'`
	if test ! "`file $inst | grep 'shell script' 2>/dev/null`" ; then
	    echo "" > c 2>/dev/null
	    $INSTALL --backup=nil c d 1>/dev/null 2>/dev/null
	    if test $? -eq 0; then
	        INSTALL_BACKUP="--backup=nil"
	    fi
	    $INSTALL -C --backup=nil c e 1>/dev/null 2>/dev/null
	    if test $? -eq 0; then
	        INSTALL_BACKUP="-C --backup=nil"
	    fi
	fi
	rm -f c d e 2>/dev/null
fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $INSTALL_BACKUP" >&5
$as_echo "$INSTALL_BACKUP" >&6; }


