		{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for liblas >= $LIBLAS_VERSION_REQUIRED_MIN" >&5
$as_echo_n "checking for liblas >= $LIBLAS_VERSION_REQUIRED_MIN... " >&6; }
