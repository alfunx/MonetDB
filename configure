  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


		if test "x$PERL" = xno; then
			if test "x$have_perl" != xauto; then
				as_fn_error $? "No Perl executable found" "$LINENO" 5
			fi
			have_perl=no
			why_have_perl="(no Perl executable found)"
		fi
	fi
