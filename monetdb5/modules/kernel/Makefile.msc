	$(CC) $(CFLAGS) $(aggr_CFLAGS) $(GENDLL) -D_CRT_SECURE_NO_WARNINGS -DLIBKERNEL -Fo"aggr.obj" -c "$(srcdir)\aggr.c"
algebra.obj: "$(srcdir)\algebra.c" "$(srcdir)\algebra.h" "$(srcdir)\..\..\..\gdk\gdk.h" "$(srcdir)\..\..\mal\mal_instruction.h" "$(srcdir)\..\..\mal\mal_type.h" "$(srcdir)\..\..\mal\mal.h" "$(srcdir)\..\..\mal\..\..\gdk\gdk.h" "$(srcdir)\..\..\mal\mal_stack.h" "$(srcdir)\..\..\mal\mal_namespace.h" "$(srcdir)\..\..\mal\mal_errors.h" "$(srcdir)\..\..\mal\mal_exception.h" "$(srcdir)\..\..\mal\mal_interpreter.h"
	$(CC) $(CFLAGS) $(algebra_CFLAGS) $(GENDLL) -D_CRT_SECURE_NO_WARNINGS -DLIBKERNEL -Fo"algebra.obj" -c "$(srcdir)\algebra.c"
bat5.obj: "$(srcdir)\bat5.c" "$(srcdir)\bat5.h" "$(srcdir)\..\..\mal\..\..\gdk\gdk.h" "$(srcdir)\..\..\mal\mal.h" "$(srcdir)\..\..\..\gdk\gdk.h" "$(srcdir)\..\..\mal\mal_instruction.h" "$(srcdir)\..\..\mal\mal_type.h" "$(srcdir)\..\..\mal\mal_stack.h" "$(srcdir)\..\..\mal\mal_namespace.h" "$(srcdir)\..\..\mal\mal_errors.h" "$(srcdir)\..\..\mal\mal_exception.h"
	$(CC) $(CFLAGS) $(bat5_CFLAGS) $(GENDLL) -D_CRT_SECURE_NO_WARNINGS -DLIBKERNEL -Fo"bat5.obj" -c "$(srcdir)\bat5.c"
group.obj: "$(srcdir)\group.c" "$(srcdir)\..\..\mal\..\..\gdk\gdk.h" "$(srcdir)\..\..\mal\mal.h" "$(srcdir)\..\..\mal\mal_instruction.h" "$(srcdir)\..\..\mal\mal_type.h" "$(srcdir)\..\..\mal\mal_stack.h" "$(srcdir)\..\..\mal\mal_namespace.h" "$(srcdir)\..\..\mal\mal_errors.h" "$(srcdir)\..\..\mal\mal_exception.h" "$(srcdir)\group.h" "$(srcdir)\..\..\..\gdk\gdk.h" "$(srcdir)\algebra.h" "$(srcdir)\..\..\mal\mal_interpreter.h"
	$(CC) $(CFLAGS) $(group_CFLAGS) $(GENDLL) -D_CRT_SECURE_NO_WARNINGS -DLIBKERNEL -Fo"group.obj" -c "$(srcdir)\group.c"
microbenchmark.obj: "$(srcdir)\microbenchmark.c" "$(srcdir)\..\..\mal\..\..\gdk\gdk.h" "$(srcdir)\..\..\mal\mal.h" "$(srcdir)\..\..\mal\mal_instruction.h" "$(srcdir)\..\..\mal\mal_type.h" "$(srcdir)\..\..\mal\mal_stack.h" "$(srcdir)\..\..\mal\mal_namespace.h" "$(srcdir)\..\..\mal\mal_errors.h" "$(srcdir)\..\..\mal\mal_exception.h" "$(srcdir)\microbenchmark.h"
	$(CC) $(CFLAGS) $(microbenchmark_CFLAGS) $(GENDLL) -D_CRT_SECURE_NO_WARNINGS -DLIBKERNEL -Fo"microbenchmark.obj" -c "$(srcdir)\microbenchmark.c"
status.obj: "$(srcdir)\status.c" "$(srcdir)\..\..\..\gdk\gdk.h" "$(srcdir)\..\..\mal\mal_instruction.h" "$(srcdir)\..\..\mal\mal_type.h" "$(srcdir)\..\..\mal\mal.h" "$(srcdir)\..\..\mal\..\..\gdk\gdk.h" "$(srcdir)\..\..\mal\mal_stack.h" "$(srcdir)\..\..\mal\mal_namespace.h" "$(srcdir)\..\..\mal\mal_errors.h" "$(srcdir)\..\..\mal\mal_exception.h" "$(srcdir)\status.h"
	$(CC) $(CFLAGS) $(status_CFLAGS) $(GENDLL) -D_CRT_SECURE_NO_WARNINGS -DLIBKERNEL -Fo"status.obj" -c "$(srcdir)\status.c"
logger.obj: "$(srcdir)\logger.c" "$(srcdir)\..\..\..\gdk\gdk.h" "$(srcdir)\..\..\..\gdk\gdk_logger.h" "$(srcdir)\..\..\mal\..\..\gdk\gdk.h" "$(srcdir)\..\..\mal\mal.h" "$(srcdir)\..\..\mal\mal_instruction.h" "$(srcdir)\..\..\mal\mal_type.h" "$(srcdir)\..\..\mal\mal_stack.h" "$(srcdir)\..\..\mal\mal_namespace.h" "$(srcdir)\..\..\mal\mal_errors.h" "$(srcdir)\..\..\mal\mal_exception.h"
	$(CC) $(CFLAGS) $(logger_CFLAGS) $(GENDLL) -D_CRT_SECURE_NO_WARNINGS -DLIBKERNEL -Fo"logger.obj" -c "$(srcdir)\logger.c"
batstr.obj: "$(srcdir)\batstr.c" "$(srcdir)\..\..\..\gdk\gdk.h" "$(srcdir)\..\..\mal\mal_instruction.h" "$(srcdir)\..\..\mal\mal_type.h" "$(srcdir)\..\..\mal\mal.h" "$(srcdir)\..\..\mal\..\..\gdk\gdk.h" "$(srcdir)\..\..\mal\mal_stack.h" "$(srcdir)\..\..\mal\mal_namespace.h" "$(srcdir)\..\..\mal\mal_errors.h" "$(srcdir)\..\..\mal\mal_exception.h" "$(srcdir)\..\atoms\..\..\..\gdk\gdk.h" "$(srcdir)\..\atoms\..\..\mal\mal.h" "$(srcdir)\..\atoms\..\..\mal\mal_exception.h" "$(srcdir)\..\atoms\str.h"
	$(CC) $(CFLAGS) $(batstr_CFLAGS) $(GENDLL) -D_CRT_SECURE_NO_WARNINGS -DLIBKERNEL -Fo"batstr.obj" -c "$(srcdir)\batstr.c"
batmmath.obj: "$(srcdir)\batmmath.c" "$(srcdir)\batmmath.h" "$(srcdir)\..\..\..\gdk\gdk.h" "$(srcdir)\..\..\mal\mal_instruction.h" "$(srcdir)\..\..\mal\mal_type.h" "$(srcdir)\..\..\mal\mal.h" "$(srcdir)\..\..\mal\..\..\gdk\gdk.h" "$(srcdir)\..\..\mal\mal_stack.h" "$(srcdir)\..\..\mal\mal_namespace.h" "$(srcdir)\..\..\mal\mal_errors.h" "$(srcdir)\..\..\mal\mal_exception.h"
	$(CC) $(CFLAGS) $(batmmath_CFLAGS) $(GENDLL) -D_CRT_SECURE_NO_WARNINGS -DLIBKERNEL -Fo"batmmath.obj" -c "$(srcdir)\batmmath.c"
all-msc: "libkernel.lib" "00_aggr_hge.mal" "alarm.mal" "bat5.mal" "microbenchmark.mal" "batmmath.mal" "mmath.mal" "aggr.mal" "batstr.mal" "group.mal" "batcolor.mal" "status.mal" "algebra.mal" "logger.mal"
check-msc: all-msc "alarm.mal" "bat5.mal" "microbenchmark.mal" "batmmath.mal" "mmath.mal" "aggr.mal" "batstr.mal" "status.mal" "00_aggr_hge.mal" "batcolor.mal" "group.mal" "algebra.mal" "logger.mal"
install-msc: install-exec install-data
install-exec:  "install_alarm.mal" "install_bat5.mal" "install_microbenchmark.mal" "install_batmmath.mal" "install_mmath.mal" "install_aggr.mal" "install_batstr.mal" "install_status.mal" "install_00_aggr_hge.mal" "install_batcolor.mal" "install_group.mal" "install_algebra.mal" "install_logger.mal" 
install_alarm.mal: "alarm.mal" "$(libdir)\monetdb5"
	$(INSTALL) "alarm.mal" "$(libdir)\monetdb5\alarm.mal"
install_bat5.mal: "bat5.mal" "$(libdir)\monetdb5"
	$(INSTALL) "bat5.mal" "$(libdir)\monetdb5\bat5.mal"
install_microbenchmark.mal: "microbenchmark.mal" "$(libdir)\monetdb5"
	$(INSTALL) "microbenchmark.mal" "$(libdir)\monetdb5\microbenchmark.mal"
install_batmmath.mal: "batmmath.mal" "$(libdir)\monetdb5"
	$(INSTALL) "batmmath.mal" "$(libdir)\monetdb5\batmmath.mal"
install_mmath.mal: "mmath.mal" "$(libdir)\monetdb5"
	$(INSTALL) "mmath.mal" "$(libdir)\monetdb5\mmath.mal"
install_aggr.mal: "aggr.mal" "$(libdir)\monetdb5"
	$(INSTALL) "aggr.mal" "$(libdir)\monetdb5\aggr.mal"
install_batstr.mal: "batstr.mal" "$(libdir)\monetdb5"
	$(INSTALL) "batstr.mal" "$(libdir)\monetdb5\batstr.mal"
install_status.mal: "status.mal" "$(libdir)\monetdb5"
	$(INSTALL) "status.mal" "$(libdir)\monetdb5\status.mal"
!IF defined(HAVE_HGE)
install_00_aggr_hge.mal: "00_aggr_hge.mal" "$(libdir)\monetdb5\autoload"
	$(INSTALL) "00_aggr_hge.mal" "$(libdir)\monetdb5\autoload\00_aggr_hge.mal"
!ELSE
install_00_aggr_hge.mal:
!ENDIF
install_batcolor.mal: "batcolor.mal" "$(libdir)\monetdb5"
	$(INSTALL) "batcolor.mal" "$(libdir)\monetdb5\batcolor.mal"
install_group.mal: "group.mal" "$(libdir)\monetdb5"
	$(INSTALL) "group.mal" "$(libdir)\monetdb5\group.mal"
install_algebra.mal: "algebra.mal" "$(libdir)\monetdb5"
	$(INSTALL) "algebra.mal" "$(libdir)\monetdb5\algebra.mal"
install_logger.mal: "logger.mal" "$(libdir)\monetdb5"
	$(INSTALL) "logger.mal" "$(libdir)\monetdb5\logger.mal"
"$(libdir)\monetdb5":
	if not exist "$(libdir)\monetdb5" $(MKDIR) "$(libdir)\monetdb5"
"$(libdir)\monetdb5\autoload":
	if not exist "$(libdir)\monetdb5\autoload" $(MKDIR) "$(libdir)\monetdb5\autoload"
install-data:
